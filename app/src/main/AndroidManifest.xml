<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.resturant.recipe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Resturant recipe"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.resturant.recipe.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.resturant.recipe.Main2Activity" />
        <activity android:name="com.resturant.recipe.Main3Activity" />
        <activity android:name="com.resturant.recipe.Main4Activity" />
        <activity android:name="com.resturant.recipe.Main5Activity" />
        <activity
            android:name="com.resturant.recipe.ScrollingActivity"
            android:label="@string/title_activity_scrolling"></activity>
    </application>

</manifest>